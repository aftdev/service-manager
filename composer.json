{
  "name": "aftdev/service-manager",
  "description": "Laminas Service Manager improvements.",
  "license": "BSD-3-Clause",
  "keywords": [
    "laminas",
    "container",
    "service-manager"
  ],
  "require": {
    "php": "^8.1",
    "laminas/laminas-servicemanager": "^3.12.0",
    "psr/container": "^1.1 || ^2.0"
  },
  "autoload": {
    "psr-4": {
      "AftDev\\ServiceManager\\": "src/"
    },
    "classmap": [
      "src/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "AftDevTest\\ServiceManager\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit --colors=always"
  },
  "extra": {
    "laminas": {
      "config-provider": "AftDev\\ServiceManager\\ConfigProvider"
    }
  }
}
